{
  "name": "yassh.github.io",
  "version": "0.0.0",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "parcel": "^2.6.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2"
  },
  "private": true,
  "scripts": {
    "build": "npm run clean && parcel build 'src/**/*.html'",
    "clean": "rimraf ./dist",
    "dev": "npm run clean && parcel 'src/**/*.html'",
    "fix": "run-p fix:*",
    "fix:html": "prettier 'src/**/*.html' --write",
    "fix:js": "prettier 'src/**/*.{js,ts}' --write"
  }
}
